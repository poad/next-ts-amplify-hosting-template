{
  "name": "cdk",
  "version": "0.1.0",
  "bin": "bin/cdk.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "format-check": "rome format bin && rome format lib",
    "format": "rome format --write bin && rome format --write  lib",
    "check": "rome check bin && rome check lib",
    "check-fix": "rome check --apply-unsafe bin && rome check --apply-unsafe lib",
    "lint": "pnpm format-check && pnpm check",
    "lint-fix": "pnpm format && pnpm check-fix",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@swc/core": "^1.3.77",
    "@swc/helpers": "^0.5.1",
    "@types/node": "^20.5.0",
    "aws-cdk": "^2.92.0",
    "esbuild": "^0.19.2",
    "regenerator-runtime": "0.14.0",
    "rome": "^12.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@aws-cdk/aws-amplify-alpha": "2.90.0-alpha.0",
    "aws-cdk-lib": "^2.92.0",
    "constructs": "^10.2.69",
    "source-map-support": "^0.5.21"
  },
  "packageManager": "pnpm@8.6.12",
  "engines": {
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!",
    "pnpm": ">=8.6.12"
  }
}
