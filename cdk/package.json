{
  "name": "cdk",
  "version": "0.1.0",
  "bin": "bin/cdk.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "format-check": "biome format bin && biome format lib",
    "format": "biome format --write bin && biome format --write  lib",
    "check": "biome check bin && biome check lib",
    "check-fix": "biome check --apply-unsafe bin && biome check --apply-unsafe lib",
    "lint": "pnpm format-check && pnpm check",
    "lint-fix": "pnpm format && pnpm check-fix",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.0",
    "@types/node": "^20.8.10",
    "aws-cdk": "^2.103.1",
    "esbuild": "^0.19.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@aws-cdk/aws-amplify-alpha": "2.95.0-alpha.0",
    "aws-cdk-lib": "^2.103.1",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  },
  "engines": {
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!",
    "pnpm": ">=8.7.4"
  }
}
