    "react": "*",
    "react-dom": "*",
    "typescript": "*",
    "web-vitals": "*"
  },
  "devDependencies": {
    "rimraf": "*",
    "rome": "*"
  },
  "workspaces": [
    "./cdk"
  ],
  "packageManager": "yarn@3.5.1"
}
